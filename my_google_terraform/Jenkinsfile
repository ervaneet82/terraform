pipeline {
  agent any
  environment {
    SVC_ACCOUNT_KEY = credentials('google-terraform')
  }
  stages {
    stage('Checkout') {
      steps {
        dir('my_google_terraform') {
          withCredentials([file(credentialsId: 'google-terraform', variable: 'mygoogle')]) {
            sh 'use $FILE'
          }
        }
      }
    }
  }
}
